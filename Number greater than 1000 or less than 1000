#include<stdio.h>
int main()
{
	int number;
	printf("please enter a number:");
	scanf("%d",&number);
	if(number<10)
	{
		printf("number is less than 1000!\n");
		}
		else if(number==10)
		{
			printf("number is 1000!!\n");
			}
			else
			{
				printf("number is greater than!\n");
				}
				return 0;
				}
